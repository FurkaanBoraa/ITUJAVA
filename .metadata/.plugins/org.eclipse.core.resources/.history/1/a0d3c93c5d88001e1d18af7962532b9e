package lab7;

import java.util.Arrays;

public class L7Q1 {
	public static void unique(int[] array) {
		Arrays.sort(array);
		int check = array[0];
		int uniqueCounter=0;
		for (int i = 1; i < array.length; i++) {
			int temp = array[i];
			if(temp != check) {
				uniqueCounter++;
				check = temp;
			}
			System.out.println(uniqueCounter);
		}
	}
	public static void main(String[] args) {
		int[] array = {0,1,3,7,4,1,8,6};
		unique(array);

	}

}
