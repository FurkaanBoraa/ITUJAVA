package lab6;

public class ComplexNumber {
	private double real;
	private double imag;

	public ComplexNumber(double real, double imag) {
		this.real = real;
		this.imag = imag;
	}
	
	public double conjugate(ComplexNumber number) {
		double conjugateReal = number.getReal();
		double conjugateImag= -number.getImag();
		
	}

	public double getReal() {
		return real;
	}

	public void setReal(double real) {
		this.real = real;
	}

	public double getImag() {
		return imag;
	}

	public void setImag(double imag) {
		this.imag = imag;
	}
	
	
}
